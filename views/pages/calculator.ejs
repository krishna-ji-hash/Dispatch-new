                  // Add pagination controls if needed
                  addPaginationControls(totalPages);

                  // Update meta information
                  const cheapest = lastQuotes[0].best.total;
                  updateMeta(cheapest);
                } function updateMeta(cheapest) {

                  const count = lastQuotes.length;
