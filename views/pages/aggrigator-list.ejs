<script>
    // Prevent global DataTables from initializing our table
    window.addEventListener('load', function() {
        // Protect our table from global DataTables initialization
        if (typeof $ !== 'undefined') {
            // Temporarily override the global DataTables selector for our table
            const originalDataTable = $.fn.DataTable;
            const protectedTables = ['aggregatorTable'];

            $.fn.DataTable = function(options) {
                // Check if this is trying to initialize our protected table
                if (this.selector && protectedTables.some(tableId => this.selector.includes(tableId))) {
                    console.log('Skipping global DataTables initialization for protected table:', this.selector);
                    return this; // Return jQuery object without initializing DataTables
                }
                // For other tables, use original DataTables
                return originalDataTable.apply(this, arguments);
            };
        }
    });

