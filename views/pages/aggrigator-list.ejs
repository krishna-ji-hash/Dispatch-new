<script>
    function deleteAggregator(aggregatorId) {
        // Show confirmation dialog
        Swal.fire({
            title: 'Delete Aggregator',
            text: 'Are you sure you want to delete this aggregator?',
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#d33',
            cancelButtonColor: '#3085d6',
            confirmButtonText: 'Yes, delete it!',
            cancelButtonText: 'Cancel'
        }).then((result) => {
            if (result.isConfirmed) {
                // Show loading state
                Swal.fire({
                    title: 'Deleting...',
                    text: 'Please wait while we delete the aggregator',
                    allowOutsideClick: false,
                    didOpen: () => {
                        Swal.showLoading();
                    }
                });

                // Make API call to delete the aggregator
                axios.delete(`/api/aggrigator/${aggregatorId}`)
                    .then(response => {
                        if (response.data.success) {
                            Swal.fire({
                                icon: 'success',
                                title: 'Deleted!',
                                text: 'Aggregator has been deleted successfully.',
                                timer: 2000,
                                showConfirmButton: false
                            }).then(() => {
                                // Reload the page to reflect changes
                                window.location.reload();
                            });
                        } else {
                            Swal.fire({
                                icon: 'error',
                                title: 'Error!',
                                text: response.data.message || 'Failed to delete aggregator.'
                            });
                        }
                    })
                    .catch(error => {
                        console.error('Error deleting aggregator:', error);
                        Swal.fire({
                            icon: 'error',
                            title: 'Error!',
                            text: 'An error occurred while deleting the aggregator.'
                        });
                    });
            }
        });
    }
</script>
