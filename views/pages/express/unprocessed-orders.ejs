                  addProgressDetail(`âŒ Order ${orderId}: Order not found`, 'danger');
                  results.failed.push({ orderId, reason: 'Order not found' });
                  continue;
                }

                try {
                  // Update progress
                  const progress = ((i + 1) / checkboxes.length) * 100;
                  progressBar.style.width = `${progress}%`;
                  progressBar.textContent = `${Math.round(progress)}%`;
                  processedCount.textContent = i + 1;

                  addProgressDetail(`ðŸ”„ Processing Order ${orderId}...`, 'info');

                  // Fetch rates for this order
                  addProgressDetail(`ðŸ” Order ${orderId}: Fetching rates...`, 'info');
                  const rates = await fetchRatesForOrder(order);
                  
                  // Check if rates is an error object
                  if (rates && rates.error) {
                    addProgressDetail(`âŒ Order ${orderId}: ${rates.message}`, 'danger');
                    
                    // Add additional details if available
                    if (rates.details && rates.details.availablePartners) {
                      addProgressDetail(`ðŸ“‹ Order ${orderId}: Available partners: ${rates.details.availablePartners.join(', ')}`, 'info');
                    }
                    
                    results.failed.push({ 
                      orderId, 
                      reason: rates.message, 
                      errorType: 'courier_not_serviceable',
                      details: rates.details 
                    });
                    
                    if (stopOnError) break;
                    continue;
                  }
                  
                  if (!rates || rates.length === 0) {
                    addProgressDetail(`âŒ Order ${orderId}: No rates available`, 'danger');
                    results.failed.push({ orderId, reason: 'No rates available' });
                    if (stopOnError) break;
                    continue;
                  }
                  
                  addProgressDetail(`ðŸ“Š Order ${orderId}: Found ${rates.length} rates`, 'info');

                  // Find the best rate (lowest cost)
                  const bestRate = rates.reduce((best, current) => 
