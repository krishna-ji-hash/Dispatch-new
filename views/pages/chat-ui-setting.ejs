  <script>
    const clientId = "<%= clientId %>"; // Pass the clientId from EJS
    console.log("client id ",clientId)
   document.querySelectorAll('.form-check-input').forEach(checkbox => {
     checkbox.addEventListener('change', async function() {
       const statusType = this.getAttribute('data-status');
       const value = this.checked ? 1 : 0; // 1 for checked, 0 for unchecked
      
       // console.log("check client id comes",<%= clientId %>)
       try {
         const response = await axios.post('/api/whatsapp/update-status', {
           client_id: clientId,
           status_type: statusType,
           value: value
         });
 
         if (response.data.success) {
           console.log('Status updated successfully');
         } else {
           console.error('Failed to update status:', response.data.message);
         }
       } catch (error) {
         console.error('Error updating status:', error);
       }
     });
   });


   document.getElementById('toggle-activation-button').addEventListener('click', async function() {
   
   try {
     const response = await axios.post('/api/whatsapp/toggle-activation', {
       client_id: clientId
     });
     if (response.data.success) {
       // Update the UI based on the new status
       const newStatus = response.data.isactive;
       const statusText = newStatus === 1 ? 'üü¢ Activated' : 'üü¢ Deactivated';
       const buttonText = newStatus === 1 ? 'Deactivate' : 'Activate';
       document.getElementById('activation-status').innerHTML = statusText;
       document.getElementById('toggle-activation-button').innerHTML = buttonText;
     } else {
       console.error('Failed to toggle activation:', response.data.message);
     }
   } catch (error) {
     console.error('Error toggling activation:', error);
   }
 });

 </script>
