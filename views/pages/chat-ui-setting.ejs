<style>
  .main-container {
    padding: 20px;
  }

  .left-section {
    background-color: #fff;
    padding: 20px;
    border-radius: 10px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    height: 100%;
  }

  .right-section {
    background-color: #fff;
    padding: 20px;
    border-radius: 10px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
  }

  .status-table {
    width: 100%;
  }

  .status-table th, .status-table td {
    vertical-align: middle;
  }

  .green-text {
    color: #28a745;
    font-weight: 600;
  }

  .bold-blue {
    font-weight: bold;
    color: #3366cc;
    font-size: 1.2rem;
  }

  .btn-demo {
    width: 100%;
  }

  .tabs {
    border-bottom: 2px solid #ddd;
    margin-bottom: 15px;
  }

  .tabs button {
    background: transparent;
    border: none;
    padding: 10px 20px;
    font-weight: 600;
  }

  .tabs .active {
    background-color: #e8f0fe;
    border-radius: 5px 5px 0 0;
  }
</style>
